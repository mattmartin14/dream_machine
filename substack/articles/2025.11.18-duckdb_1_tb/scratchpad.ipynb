   "source": [
    "import duckdb\n",
    "cn = duckdb.connect()\n",
    "\n",
    "f_path = f'/Volumes/xd1/data/*.parquet'\n",
    "\n",
    "sql = f\"\"\"\n",
    "    SELECT *\n",
    "    FROM read_parquet('{f_path}')\n",
    "    LIMIT 10\n",
    "\"\"\"\n",
    "cn.sql(sql).show()\n",
    "\n",
    "cn.sql(f\"select count(*) as row_cnt from read_parquet('{f_path}')\")"
   ]
  }
