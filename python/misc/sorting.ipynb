   "source": [
    "def bub_sort(arr: list) -> list:\n",
    "    n = len(arr)\n",
    "\n",
    "    for i in range(n):\n",
    "        for j in range(0, n-i-1):\n",
    "            if arr[j] > arr[j+1]:\n",
    "                #swap them\n",
    "                arr[j+1], arr[j] = arr[j], arr[j+1]\n",
    "\n",
    "    return arr\n",
    "\n",
    "\n",
    "arr_list = [5,3,2,1,7,4]\n",
    "bub_sort(arr_list)"
   ]
  }
